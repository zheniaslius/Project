                            <path d="M12 20C12 16.134 15.134 13 19 13L29 13C32.866 13 36 16.134 36 20C36 23.866 32.866 27 29 27L19 27C15.134 27 12 23.866 12 20Z" fill="white"/>
                            <path d="M13 47C13 43.134 16.134 40 20 40H31C34.866 40 38 43.134 38 47C38 50.866 34.866 54 31 54H20C16.134 54 13 50.866 13 47Z" fill="white"/>
                            <path d="M13 75C13 71.6863 15.6863 69 19 69H32C35.3137 69 38 71.6863 38 75C38 78.3137 35.3137 81 32 81H19C15.6863 81 13 78.3137 13 75Z" fill="white"/>
                            <path d="M150 75C150 71.6863 152.686 69 156 69H168C171.314 69 174 71.6863 174 75C174 78.3137 171.314 81 168 81H156C152.686 81 150 78.3137 150 75Z" fill="white"/>
                            <path d="M150 47C150 43.134 153.134 40 157 40H167C170.866 40 174 43.134 174 47C174 50.866 170.866 54 167 54H157C153.134 54 150 50.866 150 47Z" fill="white"/>
                            <path d="M148 20C148 16.134 151.134 13 155 13L165 13C168.866 13 172 16.134 172 20C172 23.866 168.866 27 165 27L155 27C151.134 27 148 23.866 148 20Z" fill="white"/>
                            </g>
                            <circle cx="93.5" cy="47.5" r="37.5" fill="white"/>
                            <rect x="63" y="21" width="52" height="52" fill="url(#pattern0)"/>
                            <defs>
                            <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="assets/images/services/1.svg" transform="scale(0.0078125)"/>
                            </pattern>
                            <image id="image0" width="128" height="128" xlink:href="data:image/png;base64,"/>
                            </defs>
                        </svg>
                    <span class="service__name">Агрегатор доставки</span>
                    <span class="service__desc">Более 15 служб доставки в одном договоре</span>
                    <a href="" class="service_more">Узнать больше</a>
                </div>
            </div>
        </div>
    </section>
</body>

</html>
