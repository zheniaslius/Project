{
  "name": "webpack-boilerplate",
  "version": "4.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS and lodash on board",
  "author": "Christoph von Gellhorn",
  "license": "MIT",
  "homepage": "https://github.com/cvgellhorn/webpack-boilerplate",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack.config.dev.js",
    "build": "webpack -p --progress --mode production --config webpack.config.build.js",
    "deploy": "npm run build && gh-pages -d dist",
    "heroku-postbuild": "npm build"
  },
  "repository": {
    "type": "git",
    "url": "https://cvgellhorn@github.com/cvgellhorn/webpack-boilerplate.git"
  },
  "dependencies": {
    "@glidejs/glide": "^3.2.3",
    "animejs": "^2.2.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "in-view": "^0.6.1",
    "lodash": "4.17.11",
    "reset-css": "^4.0.1",
    "smooth-scroll": "^15.0.0",
    "svg-url-loader": "^2.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "babel-loader": "8.0.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "^4.5.0",
    "node-sass": "4.9.4",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  }
}
